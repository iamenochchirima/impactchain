type email = text;
type UserRecord = 
 record {
   aboutCompany:
    record {
      companySize: text;
      industry: text;
      logo: text;
      name: text;
    };
   email: text;
   impactTargets: opt vec ImpactTarget;
 };
type Result = 
 variant {
   err: text;
   ok: UserRecord;
 };
type Measurement = 
 record {
   documents: vec text;
   goal: opt text;
   iotDevice: opt IOTDevice;
   name: text;
 };
type ImpactTarget = 
 record {
   id: nat;
   measurements: vec Measurement;
   name: text;
 };
type IOTDevice = 
 record {
   id: text;
   ipAdress: text;
   name: text;
   platform: text;
 };
service : {
  addUserRecord: (UserRecord) -> ();
  getUserRecord: (email) -> (Result) query;
  removeUserRecord: (email) -> ();
  updateUserRecord: (UserRecord) -> ();
}
